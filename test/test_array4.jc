printf=__c_function(int,"printf","stdio.h")

struct int2
	x=0
	y=0
	inline __init__(v_x,v_y)
		x=v_x
		y=v_y

//this n influences the array
(function(){
	auto n=10
	auto a=int2[0]
	for(i=0;i<n;i++)
		a.push(int2(i,i*i))
	for a_i,I in a
		a[I].x+=1000
	for a_i in a
		printf("%d %d\n",a_i.x,a_i.y)
})()
