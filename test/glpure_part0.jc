		for slot=m_n_attr_arrays-1:-1:0
			glDisableVertexAttribArray(u32(slot))
		glUseProgram(0u)
		glBindBuffer(GL_ARRAY_BUFFER,0u)
		m_ebo_type=GL_INVALID_ENUM
		m_ebo_ofs=0L
		m_ebo=0u
		m_refs.clear()//refs could be holding vbo references
		if err_draw!=GL_NO_ERROR:
			return 0
		else
