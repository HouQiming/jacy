class CJSGLTexture
	GLuint m_texid
	auto __done__()
		if m_texid:
			texid=m_texid
			glDeleteTextures(1,&texid)
	
auto glCreateTexture(int format,int w,int h, int filter,int wraps,int wrapt,__pointer ptr_init)
	tex=0u
	glGenTextures(1, &tex)
	glBindTexture(GL_TEXTURE_2D, tex)
	glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MAG_FILTER, (filter==GL_LINEAR_MIPMAP_LINEAR||filter==GL_LINEAR_MIPMAP_NEAREST?GL_LINEAR:filter))
	glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MIN_FILTER, filter)
	glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_S, wraps)
	glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_T, wrapt)
	glGetError()
	channels=GL_RGBA
	t=GL_UNSIGNED_BYTE
	switch format{
	default:
		if Platform.BUILD=="debug":
			__basic_api.spapReportError('unsupported texture format @1'.Replace(["@1",formatNumber(format,{base:16,align:4})]))
		return 0u
	case GL_LUMINANCE:
		channels=GL_LUMINANCE
		t=GL_UNSIGNED_BYTE
		break
	case GL_RGBA32F:
		t=GL_FLOAT
		break
	case GL_RGBA16F:
		if Platform.ARCH=="ios"||Platform.ARCH=="web":
			//ios needs OES here but not when reading...
			t=GL_HALF_FLOAT_OES
		else
			t=GL_HALF_FLOAT
		break
	case GL_RGBA8:
