GL_RTT_MIPMAP=1
GL_RTT_DEPTH=2
inline glCreateFramebuffer()
	ret=[0u]
	glGenFramebuffers(1,__pointer(ret.d))
	return ret[0]

inline glCreateBuffer(what,data)
	buf_ret=[0u]
	glGenBuffers(1,__pointer(buf_ret.d))
	ret=buf_ret[0]
	glBindBuffer(what,ret)
	glBufferData(what,data.n*sizeof(data[0]),__pointer(data.d),GL_STATIC_DRAW)
	glBindBuffer(what,0u)
	return ret

auto glCreateRenderToTextureContext(GLenum format,int w,int h,int flags)
	p_old_fbo=new u32[1]
	glGetIntegerv(GL_FRAMEBUFFER_BINDING,__pointer(p_old_fbo.d))
	tex=glCreateTexture(
		format,w,h,
		GL_LINEAR,GL_CLAMP_TO_EDGE,GL_CLAMP_TO_EDGE,
		NULL)
	if flags&GL_RTT_MIPMAP:
		glBindTexture(GL_TEXTURE_2D,tex)
		glGenerateMipmap(GL_TEXTURE_2D)
		glBindTexture(GL_TEXTURE_2D,0u)
	fbo=glCreateFramebuffer()
	rbo=0u
	if flags&GL_RTT_DEPTH:
		p_old_rbo=new u32[1]
		glGetIntegerv(GL_RENDERBUFFER_BINDING_OES,__pointer(p_old_rbo.d))
		glGenRenderbuffers(1,&rbo);
		glBindRenderbuffer(GL_RENDERBUFFER,rbo);
		if Platform.ARCH=="web":
			glRenderbufferStorage(GL_RENDERBUFFER,GL_DEPTH_COMPONENT16,w,h);
		else
			glRenderbufferStorage(GL_RENDERBUFFER,GL_DEPTH24_STENCIL8,w,h);
		glBindRenderbuffer(GL_RENDERBUFFER,p_old_rbo[0]);
	glBindFramebuffer(GL_FRAMEBUFFER,fbo);
	glFramebufferTexture2D(GL_FRAMEBUFFER,GL_COLOR_ATTACHMENT0,GL_TEXTURE_2D,tex,0);
	if flags&GL_RTT_DEPTH:
		if Platform.ARCH=="web":
			glFramebufferRenderbuffer(GL_FRAMEBUFFER,GL_DEPTH_ATTACHMENT,GL_RENDERBUFFER,rbo);
		else
			glFramebufferRenderbuffer(GL_FRAMEBUFFER,GL_DEPTH_STENCIL_ATTACHMENT,GL_RENDERBUFFER,rbo);
	glBindFramebuffer(GL_FRAMEBUFFER,p_old_fbo[0]);
	return new CJSGLRenderToTextureContext(){m_tex:tex,m_rbo:rbo,m_fbo:fbo,w:w,h:h}

