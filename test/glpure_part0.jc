			m_prg=glCreateProgramFromString([TShaderItem(){what:GL_VERTEX_SHADER,scode:svs},TShaderItem(){what:GL_FRAGMENT_SHADER,scode:sfs}],fprelink)
			if !m_prg:
				m_is_invalid=1
				if Platform.BUILD=="debug":
					Writeln("Failed to compile the following shaders:\n"+svs[:svs.n-2]+'\n====================\n'+sfs[:sfs.n-2]+"\n")
				return 0
			foreach di in m_ditems
				if di.decl_code[0]!='a':
					s_native=new(di.loc_name);s_native.push(char(0))
					m_slot_map[di.loc_name]=glGetUniformLocation(m_prg,__pointer(s_native.d))
			if Platform.BUILD=="debug":
				g_n_shaders_compiled++
				if g_n_shaders_compiled>200:
