		if IS_OPENGL_ES:
			//GLES quad emulation
			if mode==GL_QUADS&&m_ebo_type==GL_INVALID_ENUM:
				mode=GL_TRIANGLES
				count=(int(count0)>>2)*6
				if count0<=65536L:
					dummy_ebo=new short[count]
					pt=0
					for i=0:(int(count0)>>2)-1
						dummy_ebo[pt+0]=short(i*4+0)
						dummy_ebo[pt+1]=short(i*4+1)
						dummy_ebo[pt+2]=short(i*4+2)
						dummy_ebo[pt+3]=short(i*4+0)
						dummy_ebo[pt+4]=short(i*4+2)
						dummy_ebo[pt+5]=short(i*4+3)
						pt+=6
					SetIndexArray(dummy_ebo)
				else
					dummy_eboi=new int[count]
					pt=0
					for i=0:(int(count0)>>2)-1
						dummy_eboi[pt+0]=int(i*4+0)
						dummy_eboi[pt+1]=int(i*4+1)
						dummy_eboi[pt+2]=int(i*4+2)
						dummy_eboi[pt+3]=int(i*4+0)
						dummy_eboi[pt+4]=int(i*4+2)
						dummy_eboi[pt+5]=int(i*4+3)
						pt+=6
					SetIndexArray(dummy_eboi)
		glGetError()//clear the error
		if m_ebo_type!=GL_INVALID_ENUM:
			glBindBuffer(GL_ELEMENT_ARRAY_BUFFER,m_ebo)
			if m_instance_count>1&&!Platform.IS_MOBILE:
				glDrawElementsInstanced(mode,count,m_ebo_type,__pointer(m_ebo_ofs),m_instance_count)
			else
				//Writeln('count=',count)
				glDrawElements(mode,count,m_ebo_type,__pointer(m_ebo_ofs))
			if Platform.BUILD=="debug":
				glFinish()
			err_draw=glGetError()
			glBindBuffer(GL_ELEMENT_ARRAY_BUFFER,0u)
		else
			if m_instance_count>1&&!Platform.IS_MOBILE:
				glDrawArraysInstanced(mode,0,count,m_instance_count)
			else
				glDrawArrays(mode,0,count)
			if Platform.BUILD=="debug":
				glFinish()
			err_draw=glGetError()
