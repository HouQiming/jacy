		glTexImage2D(GL_TEXTURE_2D, 0, format, w,h, 0, channels,t, ptr_init)
		//glTexImage2D(GL_TEXTURE_2D, 0, GL_RGBA, w,h, 0, GL_RGBA,GL_UNSIGNED_BYTE, ptr_init)
	err=glGetError()
