				if di.decl_code[0]=='a'||di.decl_code[0]=='u'&&!fs_appeared[di.loc_name]:
					//put attribute and VTF in vs
					svs.push(di.decl_code)
				else
					//put everything else in fs
					sfs.push(di.decl_code)
			svs.push(m_code_vs)
			sfs.push(m_code_fs)
			svs.push('\n')
			sfs.push('\n')
			auto deCRLF(string s0)
				auto sret=new string
				foreach ch in s0
					if ch=='\r':continue
					//if ch=='\n':sret.push('\r')
					sret.push(ch)
				return sret
			svs=deCRLF(svs)
			sfs=deCRLF(sfs)
			svs.push(char(0))
			sfs.push(char(0))
			fprelink=function(GLuint prg){
				//create slot_map for attrs and bind the attribute locations
				vloc_counter=0
				foreach di in m_ditems
					if di.decl_code[0]=='a':
