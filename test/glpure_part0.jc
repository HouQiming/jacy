	auto Declare(string cd,string name,function(int) f)
		m_ditems.push(TDeclItem(){
			decl_code:cd,
			loc_name:name,
			fset:f})
	//the caller is responsible for other GL states like blending, depth and stuff
	auto Draw(GLuint vbo, GLenum mode0,iptr count0)
		__force_alive(float2().x);__force_alive(float2().y)
		__force_alive(float3().x);__force_alive(float3().y);__force_alive(float3().z)
		__force_alive(float4().x);__force_alive(float4().y);__force_alive(float4().z);__force_alive(float4().w)
		mode=mode0
		count=int(count0)
		if m_is_invalid:
			m_ditems.clear()
