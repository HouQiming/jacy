		proto.ExportProperty(this,"m_code_fs")
		proto.ExportProperty(this,"m_code_varyings")
		proto.ExportProperty(this,"m_code_extensions")
		proto.ExportProperty(this,"m_instance_count")
		proto.ExportMethod(this,"SetUniform",JS_SetUniform)
		proto.ExportMethod(this,"SetTexture",JS_SetTexture)
		proto.ExportMethod(this,"SetVBO",JS_SetVBO)
		proto.ExportMethod(this,"SetVertexPointer",JS_SetVertexPointer)
		proto.ExportMethod(this,"Draw",JS_Draw)
	}

