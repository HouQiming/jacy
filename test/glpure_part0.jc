//'managed' GL wrapper
auto glGetShaderInfoLogString(GLuint obj)
	len=0
	glGetShaderiv(obj,GL_INFO_LOG_LENGTH,&len);
	if len>0:
		s=new char[len]
		glGetShaderInfoLog(obj,len,&len,__pointer(s.d))
		if s.n&&!s.back():s.pop()
		return s
	return ""
auto glGetProgramInfoLogString(GLuint obj)
	len=0
	glGetProgramiv(obj,GL_INFO_LOG_LENGTH,&len);
	if len>0:
		s=new char[len]
		glGetProgramInfoLog(obj,len,&len,__pointer(s.d))
		if s.n&&!s.back():s.pop()
		return s
	return ""
auto glAddShader(GLuint prg,string src,GLenum what)
	obj=glCreateShader(what)
	psrc=src.d
	glShaderSource(obj,1,__pointer(&psrc),NULL);
	glCompileShader(obj)
	if Platform.BUILD=="debug":
		err=glGetShaderInfoLogString(obj)
	compiled=0
	glGetShaderiv(obj,GL_COMPILE_STATUS,&compiled)
	//immediate error printing
	if !compiled:
		s=glGetShaderInfoLogString(obj)
		glDeleteShader(obj)
		if Platform.BUILD=="debug":
			Writeln(src,'=============== error ==============\n',s)
		return 0
	else
		glAttachShader(prg,obj)
		//if !IS_OPENGL_ES:
		glDeleteShader(obj)
		if Platform.BUILD=="debug":
			if err.n:
				Writeln(src,'=============== warning ==============\n',err)
		return 1
struct TShaderItem
	what=0
	scode=string.NULL
auto glCreateProgramFromString(TShaderItem[] strs,function(GLuint) fprelink)
	prg=glCreateProgram()
	if Platform.BUILD=="debug":
		s_compile_errors=new string
	foreach si in strs
		did=glAddShader(prg,si.scode,si.what)
		if !did:
			glDeleteProgram(prg)
			return 0u
	fprelink(prg)
	linked=0
	glLinkProgram(prg)
	glGetProgramiv(prg,GL_LINK_STATUS,&linked)
	if Platform.BUILD=="debug":
		s_compile_errors.push(glGetProgramInfoLogString(prg))
	if !linked:
		err=glGetProgramInfoLogString(prg)
		glDeleteProgram(prg)
		if Platform.BUILD=="debug":
			Writeln(strs,'=============== error ==============\n',err)
		return 0u
	else
		validated=0
		glValidateProgram(prg)
		glGetProgramiv(prg,GL_VALIDATE_STATUS,&validated)
		if Platform.BUILD=="debug":
			s_compile_errors.push(glGetProgramInfoLogString(prg))
		if !validated:
			err=glGetProgramInfoLogString(prg)
			glDeleteProgram(prg)
			if Platform.BUILD=="debug":
				Writeln(strs,'=============== error ==============\n',err)
			return 0u
		else
			if Platform.BUILD=="debug":
				s_compile_errors.push(glGetProgramInfoLogString(prg))
				if s_compile_errors.n:
					Writeln('=============== warning ==============\n',s_compile_errors)
				return prg
			else
				return prg

