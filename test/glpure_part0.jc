		GL_HEADER_FILE='glew.h'
		glewInit=__c_function(int,"glewInit",GL_HEADER_FILE)
		(inline(){glewInit()})()
	else if Platform.ARCH=='mac':
		GL_HEADER_FILE='OpenGL/gl.h'
	else if Platform.ARCH=='ios':
		GL_HEADER_FILE='OpenGLES/ES2/gl.h'
	else if Platform.ARCH=='android':
		GL_HEADER_FILE='GLES2/gl2.h'
	else
		//linux
