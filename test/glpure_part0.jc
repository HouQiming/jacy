		else
			t=GL_HALF_FLOAT
		break
	case GL_RGB8:
		channels=GL_RGB
		t=GL_UNSIGNED_BYTE
		break
	case GL_ALPHA8:
		channels=GL_ALPHA
		t=GL_UNSIGNED_BYTE
		break
	case GL_RG16F:
		channels=GL_RG
		if Platform.ARCH=="ios"||Platform.ARCH=="web":
			//ios needs OES here but not when reading...
			t=GL_HALF_FLOAT_OES
		else
			t=GL_HALF_FLOAT
		break
	case GL_R32F:
		channels=GL_RED
		t=GL_FLOAT
		break
	case GL_RG32F:
		channels=GL_RG
		t=GL_FLOAT
		break
	case GL_SRGB_EXT:
