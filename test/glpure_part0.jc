	//JS part
	JS_SetUniform=function(JSContext JS){
		name=JS.Param(0).as(string)
		if !name:return JS.ReturnError("invalid name")
		valf=JS.Param(1).as(float)
		if (System.Algorithm.__float_as_int(valf)&0x7f800000)!=0x7f800000:
			SetFloat(name,valf)
		else
			val=JS.Param(1).as(JSObject)
			lg=val.length()
			if u32(lg)>16u:
				return JS.ReturnError("invalid uniform array length")
			a=new float[lg]
			for i=0:lg-1
				a[i]=val[i].as(float)
			if lg==1:
				SetFloat(name,a[0])
			else if lg==2:
				SetFloat2(name,float2(a[0],a[1]))
			else if lg==3:
				SetFloat3(name,float3(a[0],a[1],a[2]))
			else if lg==4:
				SetFloat4(name,float4(a[0],a[1],a[2],a[3]))
			else if lg==16:
				SetMatrix(name,a)
			else
				return JS.ReturnError("invalid uniform array length")
		return 0
	}
	JS_SetTexture=function(JSContext JS){
		name=JS.Param(0).as(string)
		if !name:return JS.ReturnError("invalid name")
		s_sampler=JS.Param(1).as(string)
		if !s_sampler:return JS.ReturnError("invalid sampler name")
		what=JS.Param(2).as(GLenum)
		texobj=JS.Param(3).as(CJSGLTexture)
		if !texobj:return JS.ReturnError("invalid texture")
		this.SetTexture(name,s_sampler,what,texobj.m_texid)
		return 0
	}
	//////////
	u8[] m_js_cpu_vbo
	int2[] m_js_offset_checker
	JS_SetVBO=function(JSContext JS){
		if m_js_offset_checker:
			return JS.ReturnError("you can only specify one VBO for each draw call")
		buf=JS.Param(0).as(u8[])
		//allow nulling it out
		m_js_cpu_vbo=buf
		return 0
	}
	//leave the buffer overflow unchecked -- it's trusted code for now
	JS_SetVertexPointer=function(JSContext JS){
		name=JS.Param(0).as(string)
		if !name:return JS.ReturnError("invalid name")
		dim=JS.Param(1).or(4)
		type=JS.Param(2).or(GL_FLOAT)
		normalized=JS.Param(3).or(type!=GL_FLOAT)
		stride=JS.Param(4).or(0)
		ofs=JS.Param(5).or(0)
		if !m_js_cpu_vbo:
			return JS.ReturnError("please call SetVBO first")
		SetVertexPointer(name,dim,type,normalized,iptr(stride),iptr(ofs)+m_js_cpu_vbo.d)
		//a weak check, it is NOT SECURE!
		if !m_js_offset_checker:
			m_js_offset_checker=new int2[]
		m_js_offset_checker.push(int2(stride,ofs))
		return 0
	}
	JS_Draw=function(JSContext JS){
		if !m_js_cpu_vbo:
			return JS.ReturnError("please call SetVBO first")
		//GLuint vbo
		mode0=JS.Param(0).or(GL_TRIANGLES)
		count0=JS.Param(1).or(0)
		foreach chk in m_js_offset_checker
			if uptr(chk.x*(count0-1)+chk.y)>=uptr(m_js_cpu_vbo.n*sizeof(int)):
				return JS.ReturnError("out-of-bound VBO access")
		ret=Draw(0u,mode0,count0)
		m_js_offset_checker=int2[].NULL
		return JS.Return(ret)
	}
	__JS_prototype=function(JSObject proto){
