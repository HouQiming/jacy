		if !m_prg:
			//build shader
			fs_appeared=new int[string]
			code_fs=m_code_fs
			plast=0L
			foreach ch,I in code_fs
				isalnum=(ch>='a'&&ch<='z'||ch>='A'&&ch<='Z'||ch>='0'&&ch<='9'||ch=='_'||ch>=128)
				if !isalnum:
					if plast<I:
						fs_appeared[code_fs[plast:I-1]]=1
					plast=I+1
			if plast<code_fs.n:
				fs_appeared[code_fs[plast:code_fs.n-1]]=1
			svs=new string
			sfs=new string
			svs.push(m_code_extensions)
			sfs.push(m_code_extensions)
